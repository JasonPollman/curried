{
  "private": true,
  "scripts": {
    "bench": "NODE_ENV=production ./scripts/bench",
    "build": "npm run clean && npm run transpile",
    "clean": "npx rimraf ./packages/**/dist",
    "link": "npx lerna link",
    "lint": "npx eslint packages/**/src --fix",
    "prepare": "npm run lint && npm run test && npm run build",
    "test": "NODE_ENV=test npx jest ./packages/${PKG:-**}/**/*.test.js",
    "test:cover": "npm run test -- --coverage",
    "transpile": "npx babel-node ./scripts/transpile.js"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/node": "^7.0.0",
    "@babel/plugin-transform-modules-commonjs": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^23.6.0",
    "babel-preset-minify": "^0.5.0",
    "benchmark": "^2.1.4",
    "bluebird": "^3.5.2",
    "chalk": "^2.4.1",
    "eslint": "^5.7.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "jest": "^23.6.0",
    "lerna": "^3.4.3",
    "lodash": "^4.17.11",
    "p-compose": "^1.0.1",
    "rambda": "^1.2.6",
    "rimraf": "^2.6.2"
  }
}
