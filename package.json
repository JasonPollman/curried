  "scripts": {
    "bench": "NODE_ENV=production npx babel-node ./scripts/bench.js",
    "build": "npm run clean && npm run build:all && npm run build:transpile && npm run link && npm run build:bundle && npm run build:docs",
    "build:all": "npx babel-node ./scripts/make-all.js",
    "build:bundle": "npx babel-node ./scripts/bundle.js",
    "build:docs": "npx babel-node ./scripts/build-docs.js",
    "build:versions": "npx babel-node ./scripts/build-versions.js",
    "build:transpile": "npx babel-node ./scripts/transpile.js",
    "clean": "npm run clean:nuke && npm run clean:dist",
    "clean:dist": "npx rimraf ./packages/**/dist ./dist",
    "clean:nuke": "npx rimraf ./packages/**/node_modules",
    "link": "npx lerna link",
    "lint": "npx eslint packages/**/src packages/**/bench.js --fix",
    "prepare": "npm run lint && npm run test:cover && npm run build",
    "test": "NODE_ENV=test npx jest ./packages/${PKG:-**}/**/*.test.js",
    "test:cover": "npm run test -- --coverage",
    "open:umd": "open ./resources/umd-loader.html",
    "open:coverage": "open ./coverage/lcov-report/index.html",
    "serve": "serve . -p 3000"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/node": "^7.0.0",
    "@babel/plugin-transform-modules-commonjs": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-add-module-exports": "^1.0.0",
    "babel-preset-minify": "^0.5.0",
    "benchmark": "^2.1.4",
    "bluebird": "^3.5.2",
    "chalk": "^2.4.1",
    "eslint": "^5.8.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "fs-extra-promise": "^1.0.1",
    "glob": "^7.1.3",
    "jest": "^23.6.0",
    "jsdoc-api": "^4.0.3",
    "lerna": "^3.4.3",
    "lodash": "^4.17.11",
    "p-compose": "^1.0.1",
    "rambda": "^1.2.6",
    "rimraf": "^2.6.2",
    "serve": "^10.0.2",
    "source-map-loader": "^0.2.4",
    "webpack": "^4.25.1"
  }
}
